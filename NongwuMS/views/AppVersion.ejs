<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="renderer" content="webkit">
  <title>数据管理员管理</title>
  <script charset="utf-8" src="/javascripts/jquery-2.2.1.js"></script>
  <link rel='stylesheet' href='/stylesheets/bootstrap/css/bootstrap.css' />
  <link rel='stylesheet' href='/stylesheets/index.css' />
  <script charset="utf-8" src="/stylesheets/bootstrap/js/bootstrap.js"></script>
  <script type="text/javascript" src="/javascripts/jquery.md5.js"></script>
  <script charset="utf-8" src="/javascripts/admin/user_index.js"></script>

  <script type="text/javascript">
  $(function(){
    $('#btnupload').click(function(){
		$('#inputfile').val('');
      $('#inputfile').click();
    });
    $('#inputfile').change(function(){
      if($('#inputEmail3').val().length == 0){
	  
        $('#inputEmail3').parent().parent().addClass('has-error');
        return;
      }
      $('form').submit();
    });
  });
  </script>

</head>
<body>
  <nav class="navbar navbar-default ">
    <div class="container-fluid">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="#">平原村务公开</a>
      </div>
      
      <% if(isSuperAdmin==1) {%>
      <% show = "block"%>
      <% } else {%>
      <% show = "none"%>
      <% }%>
      <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse navbar-collapse top-nav" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav nav_list">
          <li>
            <a href="/admin_dataman" style="display:<%=show%>
              ">
              用户管理
              <span class="sr-only">(current)</span>
            </a>
          </li>
          <li  >
            <a href="/admin_region" style="display:<%=show%>">乡镇管理</a>
          </li>
          <li >
            <a href="/admin_policy" style="display:<%=show%>">惠农政策</a>
          </li>
          <li>
            <a href="/admin_project" style="display:<%=show%>">惠农项目</a>
          </li>
          <li>
            <a href="/admin_event">村务公开</a>
          </li>
          <li class="active">
            <a href="/admin_version" style="display:<%=show%>">客户端版本</a>
          </li>

        </ul>

        <ul class="nav navbar-nav navbar-right">
          <li>
            <a href="#" style="padding-right: 0px">
              <%= username %></a>

          </li>
          <li style="margin-left: 0px">
            <a href="/admin_login/logout" style="color:#337AB7">注销</a>
          </li>

        </ul>
      </div>
    </div>
  </nav>

  <div class="container content" >
    <div style="height:50px;"></div>

    <div class="alert alert-success text-center" id="u_error_info"  role="alert">
    
    <% if(!version['version']){ %>
    <font style="font-size: 20px">请上传应用</font></div>
    <%}else{%>
    <font style="font-size: 20px">当前客户端版本号 : &nbsp;<code><%= version['version']%></code></font></div>
    <%}%>
    <!-- <button  type="button" class="btn btn-success" data-toggle="modal" data-target="#myModal" style="float:right;">新增用户</button>
  -->
  <div style="height:50px;"></div>
  <form class="form-horizontal" action="/admin_version" method="POST" enctype="multipart/form-data">
  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label">新的版本号</label>
    <div class="col-sm-10">
      <input name="version" type="text" class="form-control" id="inputEmail3" 
      placeholder="这里是即将上传的app的版本号" >
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
     <input accept=".apk" name="file" type="file" name="uploadapk" id="inputfile" style="display: none">
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="button" id="btnupload" class="btn btn-primary btn-lg" >选择并上传apk</button>
    </div>
  </div>
</form>

  

</div>

</body>

</html>